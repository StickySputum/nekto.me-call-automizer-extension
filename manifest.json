{
    "manifest_version": 2,
    "name": "Nekto.me call restarter",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }